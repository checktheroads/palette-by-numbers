  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": "checktheroads <matt@allroads.io>",
  "license": "BSD-3-Clause-Clear",
  "repository": {
    "type": "git",
    "url": "https://github.com/checktheroads/palette-by-numbers"
  },
  "bugs": {
    "url": "https://github.com/checktheroads/palette-by-numbers/issues"
  },
  "homepage": "https://github.com/checktheroads/palette-by-numbers#readme",
  "keywords": [
    "theme",
    "color",
    "palette",
    "generator",
    "chakra",
    "chakra-ui",
    "tailwind",
    "css"
  ],
  "files": [
    "dist/",
    "esm/"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
