    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build:cjs": "tsc",
    "lint": "eslint src/**/*.ts",
